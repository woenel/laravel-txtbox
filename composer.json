{
    "name": "woenel/laravel-txtbox",
    "description": "TxtBox API for Laravel.",
    "require": {
        "php": "^7.2|8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ronnel Martinez",
            "email": "ronnel@mronnel.com",
            "homepage": "https://mronnel.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Woenel\\TxtBox\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Woenel\\TxtBox\\TxtBoxServiceProvider"
            ],
            "aliases": {
                "TxtBox": "Woenel\\TxtBox\\Facades\\TxtBox"
            }
        }
    }
}
